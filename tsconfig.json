{
  "compilerOptions": {
    "target": "ES2020",   //자바스크립트 버전
    "module": "CommonJS", //모듈 시스템
    "outDir": "./dist",  // 컴파일 결과물 저장 경로
    "rootDir": "./", // 컴파일 할 파일 경로
    "strict": false, // 엄격한 타입 검사
    "esModuleInterop": true, // ES 모듈 호환성
    "skipLibCheck": true // 라이브러리 검사 건너뛰기
  },
  "include": ["src"], // 컴파일 할 파일 경로
  "exclude": ["node_modules"] // 컴파일 제외할 파일 경로
}